{
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=MoodMedia;Integrated Security=True;MultipleActiveResultSets=True"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApiKey": "your-development-api-key-should-be-changed-in-production",
  "RateLimit": {
    "RequestLimit": 100,
    "TimeWindowSeconds": 60
  },
  "ResponseCompression": {
    "EnableForHttps": true,
    "MimeTypes": [
      "application/json",
      "text/plain"
    ]
  }
}
